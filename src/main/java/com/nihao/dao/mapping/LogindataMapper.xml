<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nihao.dao.LogindataMapper" >
  <resultMap id="BaseResultMap" type="com.nihao.model.Logindata" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="loginname" property="loginname" jdbcType="VARCHAR" />
    <result column="ltype" property="ltype" jdbcType="VARCHAR" />
    <result column="cdatetime" property="cdatetime" jdbcType="TIMESTAMP" />
    <result column="ip" property="ip" jdbcType="VARCHAR" />
    <result column="useragent" property="useragent" jdbcType="VARCHAR" />
  </resultMap>
  
  <insert id="insertOne" parameterType="com.nihao.model.Logindata">
  insert into logindata(loginname,ltype,cdatetime,ip,useragent) 
  values(#{loginname,jdbcType=VARCHAR},#{ltype,jdbcType=VARCHAR}
  ,#{cdatetime,jdbcType=TIMESTAMP},#{ip,jdbcType=VARCHAR},#{useragent,jdbcType=VARCHAR})
  </insert>
  
</mapper>