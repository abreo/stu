<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nihao.dao.TestMapper" >
<select id="selectById" parameterType="java.lang.String" resultType="com.nihao.model.Test">
  select * from test where id=#{id}
</select>
  <delete id="deleteById" parameterType="java.lang.String">
    delete from test where id=#{id}
  </delete>
  <update id="update" parameterType="com.nihao.model.Test">
    UPDATE test set name=#{name} where id=#{id}
  </update>
  <insert id="insert" parameterType="com.nihao.model.Test">
    insert into test values(#{id},#{name})
  </insert>
</mapper>